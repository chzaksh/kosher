                    <input type="checkbox" class="form-check-input" id="chabad" <%= chabad%>>
                    <label for="" class="form-check-label"> קרוב לבית חב"ד / בית כנסת פעיל</label>
                </div>
                <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="foodKosher" <%= foodKosher%>>
                    <label for="" class="form-check-label"> אוכל כשר</label>
                </div>
                <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="airuve" <%= airuve%>>
                    <label for="" class="form-check-label"> עירוב</label>
                </div>
                <div class="form-group">
                    <select id="star" class="selectpicker">
                       <% if(!star){%>
                        <option value="" data-content="<i class='fas fa-star'>star</i>"> </option><%}%>
                        <%for(let i=1; i < 6; i++){%>
                        <option <% if(i == star ){%>selected <%}%> value="<%=i%>"
                            data-content="<i class='fas fa-star'><%=i%></i>"></option>
                        <%}%>
                    </select>
                </div>
                <div class="form-group">
                    <select class="selectpicker" id="rating">
                        <% if(!rating){%>
                            <option value="" data-content="<i class='fas fa-heart'>rating</i>"> </option><%}%>
                            <%for(let i=6; i < 10; i++){%>
                            <option <% if(i == rating ){%>selected <%}%> value="<%=i%>"
                                data-content="<i class='fas fa-heart'><%=i%>+</i>"></option>
                            <%}%>
                       
                    </select>
                </div>
            </div>
            <div id="clearBTN" class="btn btn-primary">נקה חיפוש</div>
