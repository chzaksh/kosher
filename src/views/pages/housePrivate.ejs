                    <input type="checkbox" class="form-check-input" id="air-conditioner" <%= air%>>
                    <label for="" class="form-check-label">מזגן נשלט</label>
                </div>


                <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="freezer" <%= freezer%>>
